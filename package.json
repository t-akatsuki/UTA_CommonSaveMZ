  "devDependencies": {
    "@types/node": "13.11.0",
    "@types/pako": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "cpx": "^1.5.0",
    "cross-var": "^1.1.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "localforage": "^1.9.0",
    "npm-run-all": "^4.1.5",
    "pixi.js": "5.2.4",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.7"
  }
}
